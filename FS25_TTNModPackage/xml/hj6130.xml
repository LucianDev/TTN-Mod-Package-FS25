        </dashboards>
    </drivable>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits>
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="rice" capacity="50000" foldMinLimit="1" foldMaxLimit="1"/>
                    <fillUnit unitTextOverride="$l10n_unit_literShort" showOnHud="false" showInShop="false" fillTypes="diesel" capacity="250">
                        <exactFillRootNode node="exactFillRootNodeFuel" />
                        <dashboard displayType="SLIDER" valueType="fillLevel" node="fuelSlider_decal" groups="MOTOR_ACTIVE"/>
                    </fillUnit>
                    <fillUnit unitTextOverride="$l10n_unit_literShort" showOnHud="false" showInShop="false" fillTypes="def" capacity="250">
                        <dashboard displayType="SLIDER" valueType="fillLevel" node="defSlider_decal" groups="MOTOR_ACTIVE"/>
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <foldable>
        <foldingConfigurations>
            <foldingConfiguration>
                <foldingParts startMoveDirection="1" foldMiddleAnimTime="1">
                    <foldingPart animationName="folding" speedScale="1" />
                </foldingParts>
            </foldingConfiguration>
        </foldingConfigurations>
    </foldable>

    <animations>
        <animation name="foldPipe" soundVolumeFactor="1.4">
            <part node="pipeRot2" startTime="0" endTime="1.0" startRot="0 0 0" endRot="0 0 20" />
            <part node="pipeRot2" startTime="1.5" endTime="3.5" startRot="0 0 20" endRot="0 0 20" />
            <part node="pipeRot1" startTime="0.5" endTime="3.5" startRot="0 0 0" endRot="0 -120 0" />

            <sound template="hingeImp"      startTime="0.01" direction="-1" volumeScale="1.2" pitchScale="0.6" linkNode="pipeRot2"/>
            <sound template="vibratingImp"  startTime="3.49" direction="1"  volumeScale="1.8" pitchScale="1.2" linkNode="pipeRot2"/>
            <sound template="cylinderPunch" startTime="3.49" direction="1"  volumeScale="0.8" pitchScale="2.5" linkNode="pipeRot2"/>
        </animation>

        <animation name="joystickMovement">
            <part node="joystick02" startTime="0.00" endTime="0.49" startTrans="0 0 0" endTrans="0 0 0.085"/>
            <part node="joystick02" startTime="0.49" endTime="0.51" startTrans="0 0 0.085" endTrans="-0.015 0 0.085"/>
            <part node="joystick02" startTime="0.51" endTime="1.00" startTrans="0 0 0.085" endTrans="-0.015 0 0.166"/>
        </animation>
        <animation name="rpmNeedle">
            <part node="rpmNeedle_decal" startTime="0" endTime="1" startRot="0 0 23.9829" endRot="0 0 6.630"/>
            <part node="rpmNeedle_decal" startTime="1" endTime="3" startRot="0 0 6.630" endRot="0 0 -156.724"/>
        </animation>

        <animation name="wiperFrontAnimation">
            <part node="wiper" startTime="0.0" endTime="0.5" startRot="0 0 0" endRot="0 0 -80" />
            <part node="wiper" startTime="0.5" endTime="1.0" startRot="0 0 -80" endRot="0 0 -0" />

            <sound template="wiper1" startTime="0" endTime="1" direction="1"   volumeScale="1" pitchScale="0.7" fadeOut="0.1" linkNode="wiper"/>
            <sound template="wiper2" startTime="0" endTime="1" direction="-1"  volumeScale="1" pitchScale="0.7" fadeOut="0.1" linkNode="wiper"/>
        </animation>
        <animation name="folding" soundVolumeFactor="2.2">
            <part node="componentJointHeader" startTime="0" endTime="1" startRot="0 0 0" endRot="-12 0 0"/>
            <part componentJointIndex="1" startTime="0" endTime="1" startRotMinLimit="-10 0 0" startRotMaxLimit="2 0 0" endRotLimit="0 0 0"/>
            <part node="sideProtectorHydraulic" startTime="0" endTime="1" startTrans="0.62 -0.644 0.679" endTrans="0.48 -0.644 0.679"/>
            <part node="sideProtectorArm02Ref" startTime="0" endTime="1" startTrans="1.013 0.519 -0.85" endTrans="1.013 0.519 -0.78"/>

