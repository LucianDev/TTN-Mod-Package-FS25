                    </wheel>
                    <wheel>
                        <physics tipOcclusionAreaGroupId="1" rotSpeed="1" restLoad="0.9" radius="0.316" width="0.45" repr="wheelFrontRight"  forcePointRatio="0.25" initialCompression="25" suspTravel="0.10" spring="35" damper="25" frictionScale="2" supportsWheelSink="false"/>
                    </wheel>
                    <wheel>
                        <physics tipOcclusionAreaGroupId="1" rotSpeed="0" restLoad="0.9" radius="0.316" width="0.45" repr="wheelMiddleLeft"  forcePointRatio="0.25" initialCompression="25" suspTravel="0.10" spring="35" damper="25" frictionScale="2" supportsWheelSink="false"/>
                    </wheel>
                    <wheel>
                        <physics tipOcclusionAreaGroupId="1" rotSpeed="0" restLoad="0.9" radius="0.316" width="0.45" repr="wheelMiddleRight" forcePointRatio="0.25" initialCompression="25" suspTravel="0.10" spring="35" damper="25" frictionScale="2" supportsWheelSink="false"/>
                    </wheel>
                    <wheel hasTireTracks="true" hasParticles="true" >
                        <physics tipOcclusionAreaGroupId="1" rotSpeed="1" restLoad="0.9" radius="0.316" width="0.45" repr="wheelBackLeft"    forcePointRatio="0.25" initialCompression="25" suspTravel="0.10" spring="35" damper="25" frictionScale="2" supportsWheelSink="false"/>
                        <tire tireTrackAtlasIndex="5"/>
                    </wheel>
                    <wheel hasTireTracks="true" hasParticles="true" >
                        <physics tipOcclusionAreaGroupId="1" rotSpeed="1" restLoad="0.9" radius="0.316" width="0.45" repr="wheelBackRight"   forcePointRatio="0.25" initialCompression="25" suspTravel="0.10" spring="35" damper="25" frictionScale="2" supportsWheelSink="false"/>
                        <tire tireTrackAtlasIndex="5"/>
                    </wheel>
                </wheels>
                <crawlers>
                    <crawler filename="$data/shared/crawlers/iseki/hj6130Crawler/hj6130Crawler.xml" linkWheelNodes="wheelBackLeft wheelFrontLeft" wheelNodes="wheelMiddleLeft" isLeft="true"/>
                    <crawler filename="$data/shared/crawlers/iseki/hj6130Crawler/hj6130Crawler.xml" linkWheelNodes="wheelBackRight wheelFrontRight" wheelNodes="wheelMiddleRight" isLeft="false"/>
                </crawlers>
            </wheelConfiguration>
        </wheelConfigurations>

        <ackermannSteeringConfigurations>
            <ackermannSteering rotSpeed="70" rotMax="60" rotCenter="0 0" />
        </ackermannSteeringConfigurations>
    </wheels>

    <powerConsumer ptoRpm="470" neededMaxPtoPower="10"/>

    <lights>
        <states>
            <state lightTypes="0"/>
            <state lightTypes="0 1"/>
            <state lightTypes="0 1 2"/>
            <state lightTypes="0 1 2 4"/>
        </states>

        <realLights>
            <low>
                <light node="frontLightLow" lightTypes="0" excludedLightTypes="2 3"/>
                <light node="workLightBackLow" lightTypes="1"/>
                <light node="workLightFrontLow" lightTypes="2"/>
                <light node="workLightPipeLow" lightTypes="4"/>
                <light node="highBeamLow" lightTypes="3"/>
            </low>
            <high>
                <light node="frontLightHigh" lightTypes="0" excludedLightTypes="2 3"/>
                <light node="backLightsHigh" lightTypes="0"/>
                <light node="workLightBackHigh" lightTypes="1"/>
                <light node="workLightFrontHigh" lightTypes="2"/>
                <light node="workLightPipeHigh" lightTypes="4"/>
                <light node="highBeamHigh" lightTypes="3"/>
                <brakeLight node="backLightsHigh"/>
                <turnLightLeft node="turnLightLeftFront"/>
                <turnLightRight node="turnLightRightFront"/>
                <interiorLight node="interiorScreenLight"/>
                <reverseLight node="reverseLightHigh"/>
            </high>
        </realLights>

        <staticLightCompounds>
            <staticLightCompound>
                <node node="frontLightHeader_static" intensity="5"/>
                <node node="frontLightHeaderGlass_static" intensity="1"/>
                <node node="frontLight_static" intensity="5"/>
                <node node="frontLightGlass_static" intensity="1"/>
                <node node="pipeLight_static" intensity="5"/>
                <node node="pipeLightGlass_static" intensity="1"/>
            </staticLightCompound>
        </staticLightCompounds>

        <dashboards>
            <dashboard displayType="EMITTER" valueType="turnLightLeft" node="turnLightLeftIcon_decal" emitColor="GREEN" intensity="0.3"/>
            <dashboard displayType="EMITTER" valueType="turnLightRight" node="turnLightRightIcon_decal" emitColor="GREEN" intensity="0.3"/>
        </dashboards>
    </lights>

    <licensePlates>
        <licensePlate node="licensePlateBack"  position="BACK"  preferedType="SQUARISH" placementArea="0.15 0.3 0.06 0.3"/>
    </licensePlates>

    <enterable isTabbable="true" >
        <enterReferenceNode node="hj6130_main_component1" />
        <exitPoint node="exitPoint" />

        <cameras>
            <camera node="outdoorCamera" rotatable="true" rotateNode="outdoorCameraTarget" limit="true" useWorldXZRotation="true" rotMinX="-1.4" rotMaxX="1" transMin="3.6" transMax="40" translation="0 0 4.9" rotation="-20 180 0" >
                <raycastNode node="cameraRaycastNode1"/>
                <raycastNode node="cameraRaycastNode2"/>
                <raycastNode node="cameraRaycastNode3"/>
            </camera>
            <camera node="indoorCamera" rotation="-30 180 0" rotatable="true" limit="true" rotMinX="-1.1" rotMaxX="0.4" transMin="0" transMax="0" useMirror="true" isInside="true" shadowFocusBox="shadowFocusBox" positionSmoothingParameter="0.45"/>
        </cameras>

        <mirrors>
            <mirror node="mirrorDecal01" prio="1" />
            <mirror node="mirrorDecal02" prio="1" />
        </mirrors>

        <characterNode node="playerSkin" cameraMinDistance="0.5" >
            <target ikChain="rightFoot" targetNode="playerRightFootTarget" />
            <target ikChain="leftFoot"  targetNode="playerLeftFootTarget" />
            <target ikChain="rightArm"  targetNode="playerRightHandTarget" poseId="narrowFingers"/>
            <target ikChain="leftArm"   targetNode="playerLeftHandTarget" poseId="narrowFingers"/>
        </characterNode>
    </enterable>

    <motorized>
        <consumerConfigurations>
            <consumerConfiguration>
                <consumer fillUnitIndex="2" usage="52" fillType="diesel" />  <!-- usage is per hour at max load and max rpm -->
                <consumer fillUnitIndex="3" usage="4.8" fillType="def" />  <!-- usage is per hour at max load and max rpm -->
            </consumerConfiguration>
        </consumerConfigurations>

        <differentialConfigurations>
            <differentialConfiguration>
                <differentials>
                    <differential torqueRatio="0.5" maxSpeedRatio="1.9" wheelIndex1="1" wheelIndex2="2"/> <!-- front left-right -->
                    <differential torqueRatio="0.5" maxSpeedRatio="3.3" wheelIndex1="3" wheelIndex2="4"/> <!-- middle left-right -->
                    <differential torqueRatio="0.5" maxSpeedRatio="2.0" wheelIndex1="5" wheelIndex2="6"/> <!-- back left-right -->
                    <differential torqueRatio="0.5" maxSpeedRatio="1.4" differentialIndex1="1" differentialIndex2="2" />
                    <differential torqueRatio="0.5" maxSpeedRatio="1.2" differentialIndex1="3" differentialIndex2="4" />
                </differentials>
            </differentialConfiguration>
        </differentialConfigurations>

        <motorConfigurations>
            <motorConfiguration name="HJ6130" hp="130">
                <motor torqueScale="6.548" minRpm="850" maxRpm="2200" maxForwardSpeed="30" maxBackwardSpeed="20" brakeForce="10" lowBrakeForceScale="0.1" dampingRateScale="0.6">
                    <torque normRpm="0.45" torque="0.9"/>
                    <torque normRpm="0.5" torque="0.97"/>
                    <torque normRpm="0.59" torque="1"/>
                    <torque normRpm="0.72" torque="1"/>
                    <torque normRpm="0.86" torque="0.88"/>
                    <torque normRpm="1" torque="0.72"/>
                </motor>
                <transmission minForwardGearRatio="30" maxForwardGearRatio="320" minBackwardGearRatio="69" maxBackwardGearRatio="320" name="$l10n_info_transmission_cvt"/>
            </motorConfiguration>
        </motorConfigurations>

        <exhaustEffects>
            <exhaustEffect node="exhaustParticle" filename="$data/effects/exhaust/exhaust.i3d" minRpmColor="0.9 0.9 0.9 0.1" maxRpmColor="0.9 0.9 0.9 0.4" minRpmScale="0.05" maxRpmScale="0.5" />
        </exhaustEffects>

        <brakeCompressor capacity="2000" fillSpeed="250" />
        <motorStartDuration>1800</motorStartDuration>

        <dashboards>
            <dashboard displayType="ANIMATION" valueType="speedDir" animName="joystickMovement" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="MULTI_STATE" valueType="ignitionState" node="keyIgnition" doInterpolation="true" groups="MOTOR_ACTIVE">
                <state value="0" rotation="0 0 0"/>
                <state value="1" rotation="0 0 -130"/>
                <state value="2" rotation="0 0 -90"/>
            </dashboard>
            <dashboard displayType="TEXT" valueType="speed" node="numbersSpeed" textColor="1 1 1 1" textSize="0.007" textMask="00.0" font="GENERIC" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="SLIDER" valueType="load" node="motorLoadSlider_decal" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="SLIDER" valueType="motorTemperature" node="tempSlider_decal" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="ANIMATION" valueType="rpm" animName="rpmNeedle" minValueAnim="0" maxValueAnim="3000" groups="MOTOR_ACTIVE"/>
        </dashboards>
    </motorized>

    <drivable>
        <idleTurning allowed="true" updateSteeringWheel="true" direction="1" maxSpeed="2" lockDirection="false">
            <wheel node="wheelFrontLeft" steeringAngle="-45"/>
            <wheel node="wheelBackLeft" steeringAngle="45"/>
            <wheel node="wheelFrontRight" steeringAngle="45" inverted="true"/>
            <wheel node="wheelBackRight" steeringAngle="-45" inverted="true"/>
