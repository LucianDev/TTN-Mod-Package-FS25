            <sound template="vibratingImp"    startTime="0.01" direction="-1" volumeScale="1.5" pitchScale="1"   linkNode="header"/>
            <sound template="metalImpHigh03"  startTime="0.01" direction="-1" volumeScale="0.3" pitchScale="1"   linkNode="header"/>
            <sound template="hingeImp"        startTime="0.8"  direction="1"  volumeScale="0.7" pitchScale="0.8" linkNode="header"/>
            <sound template="vibratingImp"    startTime="0.9"  direction="1"  volumeScale="1.5" pitchScale="0.8" linkNode="header"/>
        </animation>

        <animation name="pipeRot1HoseAnimation">
            <part node="pipeRot1AnimJoint" startTime="0" endTime="1" startRot="0 0 0" endRot="-86.361 25.137 -69.842" startTrans="0.008 0.450 -0.228" endTrans="-0.186 0.450 -0.054" />
        </animation>
        <animation name="dischargeNodeAnimation">
            <part node="dischargeNode" startTime="0" endTime="1" startRot="0 0 0" endRot="32 0 0"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="pipeRot1" playSound="true" freezingPipeStates="1">
                <controls axis="AXIS_PIPE" invertAxis="false" mouseSpeedFactor="0.3" iconName="PIPE_ROTATE_X" />
                <rotation rotSpeed="20" rotAcceleration="200" rotMax="-80" rotMin="-180" startRot="-120" rotationAxis="2"/>
                <dependentAnimation name="pipeRot1HoseAnimation" rotationAxis="2" minValue="0" maxValue="-180"/>
            </movingTool>
            <movingTool node="pipeRot2" playSound="true" freezingPipeStates="1">
                <controls axis="AXIS_DOOR" invertAxis="false" mouseSpeedFactor="0.3" iconName="PIPE_END_ROTATE"/>
                <rotation rotSpeed="10" rotAcceleration="200" rotMax="35" rotMin="0" rotationAxis="3"/>
                <dependentPart node="pipeRot2Hydraulic" />
                <dependentAnimation name="dischargeNodeAnimation" rotationAxis="3" minValue="0" maxValue="35"/>
            </movingTool>

            <movingTool node="componentJointHeader">
                <componentJoint index="1"/>
            </movingTool>

            <movingTool node="sideProtectorHydraulic">
                <dependentPart node="sideProtector" />
                <dependentPart node="sideProtectorHydraulicScalePart" />
            </movingTool>
        </movingTools>

        <movingParts>
            <movingPart node="header" referencePoint="headerRef" referenceFrame="hj6130_main_component1" isActiveDirty="true" maxUpdateDistance="150">
                <dependentPart node="headerHydraulic" maxUpdateDistance="50"/>
            </movingPart>
            <movingPart node="headerHydraulic" referencePoint="headerHydraulicRef" referenceFrame="headerHydraulicRef">
                <translatingPart node="headerHydraulicPunch"/>
            </movingPart>
            <movingPart node="pipeRot2Hydraulic" referencePoint="pipeRot2HydraulicRef" referenceFrame="pipeRot2HydraulicRef">
                <translatingPart node="pipeRot2HydraulicPunch"/>
            </movingPart>

            <movingPart node="sideProtector" localReferencePoint="sideProtectorArm01" referencePoint="sideProtectorArm01Ref" referenceFrame="sideProtectorRefFrame">
                <dependentPart node="sideProtectorArm01" />
                <dependentPart node="sideProtectorArm02" />
            </movingPart>
            <movingPart node="sideProtectorArm01" referencePoint="sideProtectorArm01Ref" referenceFrame="sideProtectorArm01Ref"/>
            <movingPart node="sideProtectorArm02" referencePoint="sideProtectorArm02Ref" referenceFrame="sideProtectorArm02Ref" isActiveDirty="true" maxUpdateDistance="75"/>

            <movingPart node="sideProtectorHydraulicScalePart" referencePoint="sideProtectorHydraulicScalePartRef" referenceFrame="sideProtectorHydraulicScalePartRef" scaleZ="true"/>
        </movingParts>

        <sounds>
            <actionSound template="hydraulicClose01" volumeScale="8"   pitchScale="0.7" linkNode="pipeRot1" actionNames="TOOL_MOVE_POS TOOL_MOVE_NEG"                     nodes="pipeRot1" fadeIn="0.1" fadeOut="0.1"/>
            <actionSound template="hydraulicOpen05"  volumeScale="4"   pitchScale="1.7" linkNode="pipeRot2" actionNames="TOOL_MOVE_POS TOOL_MOVE_NEG"                     nodes="pipeRot2" fadeIn="0.1" fadeOut="0.1"/>
            <actionSound template="vibratingImp"     volumeScale="1"   pitchScale="2.2" linkNode="pipeRot1" actionNames="TOOL_MOVE_END_NEG_LIMIT TOOL_MOVE_END_POS_LIMIT" nodes="pipeRot1"/>
            <actionSound template="cylinderPunch"    volumeScale="1.5" pitchScale="2.2" linkNode="pipeRot1" actionNames="TOOL_MOVE_END_NEG_LIMIT TOOL_MOVE_END_POS_LIMIT" nodes="pipeRot1"/>
            <actionSound template="vibratingImp"     volumeScale="0.7" pitchScale="1.8" linkNode="pipeRot2" actionNames="TOOL_MOVE_END_NEG_LIMIT TOOL_MOVE_END_POS_LIMIT" nodes="pipeRot2"/>
            <actionSound template="cylinderPunch"    volumeScale="1.1" pitchScale="2.1" linkNode="pipeRot2" actionNames="TOOL_MOVE_END_NEG_LIMIT TOOL_MOVE_END_POS_LIMIT" nodes="pipeRot2"/>
            <actionSound template="metalImpHigh01"   volumeScale="0.7" pitchScale="1.8" linkNode="pipeRot2" actionNames="TOOL_MOVE_END_NEG_LIMIT TOOL_MOVE_END_POS_LIMIT" nodes="pipeRot2"/>
        </sounds>
