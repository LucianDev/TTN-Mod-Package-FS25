    <wipers>
        <wiper animName="wiperFrontAnimation" >
            <state animSpeed="0.5" animPause="2.0" />
            <state animSpeed="0.75" animPause="0.0" />
            <state animSpeed="1.0" animPause="0.0" />
        </wiper>
    </wipers>

    <turnOnVehicle turnOffIfNotAllowed="true">
        <sounds>
            <work template="harvesterChopper" volumeScale="2" pitchScale="1.6" linkNodeOffset="-0.5 0 2"/>
            <work template="strawblowerWork" volumeScale="3" pitchScale="0.6" linkNodeOffset="-0.5 0 0" fadeIn="0.5" fadeOut="0.5"/>
            <work template="carrotHarvesterBeltInside01" volumeScale="3" pitchScale="0.8" linkNodeOffset="-0.5 0 0" fadeIn="0.5" fadeOut="0.5"/>
        </sounds>
    </turnOnVehicle>

    <groundReferenceNodes>
        <groundReferenceNode node="groundReferenceNode" threshold="0.25"/>
    </groundReferenceNodes>

    <workAreas>
        <workArea type="cutter" functionName="processCutterArea">
            <area startNode="workAreaStart" widthNode="workAreaWidth" heightNode="workAreaHeight"/>
            <groundReferenceNode index="1"/>
            <testAreas autoGenerate="true" zOffset="0.5" xOffset="0" length="0.6" numAreas="6" areaWidthScale="0.25"/>
        </workArea>
        <workArea type="combineChopper" functionName="processCombineChopperArea" requiresGroundContact="false" disableBackwards="false">
            <area startNode="workAreaChopperStart" widthNode="workAreaChopperWidth" heightNode="workAreaChopperHeight" />
        </workArea>
    </workAreas>



    <combine fillUnitIndex="1" allowThreshingDuringRain="false">
        <chopper available="true" workAreaIndex="2"/>
        <swath available="false"/>
        <processing toggleTime="3" />
        <folding direction="-1"/>

        <chopperEffect>
            <effectNode effectClass="TypedMotionPathEffect" effectType="STRAW_CHOPPER" materialType="CHOPPER" linkNode="chopperCenterEffectNode">
                <motionPathEffect textureFilename="$data/vehicles/iseki/hj6130/chopperCenterEffectArray.dds" numRows="16" rowLength="18" minFade="0.04" shapeScale="0.2" maxShapeScale="2.4" />
                <!-- Note: numRows * rowLength == 24 * 12 == 16 * 18 == 288 -->
            </effectNode>

            <effectNode effectClass="ParticleEffect" effectNode="chopperSmokeEmitter" particleType="SMOKE" materialType="smokeParticle" useFruitColor="true"/>
        </chopperEffect>

        <animationNodes>
            <animationNode node="threshingDrum" rotSpeed="-2000" rotAxis="1" turnOnFadeTime="2" turnOffFadeTime="2" />
            <animationNode node="rotatingPartFeeder" rotSpeed="-2000" rotAxis="1" turnOnFadeTime="2" turnOffFadeTime="2" />
            <animationNode node="driveBelt" rotSpeed="-100" rotAxis="1" turnOnFadeTime="2" turnOffFadeTime="2" shaderComponentScale="0 1 0 0" shaderParameterName="offsetUV" />
